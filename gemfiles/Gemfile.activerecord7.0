# frozen_string_literal: true

source 'https://rubygems.org'

gemspec

gem 'activerecord', '~> 7.0.0'
gem 'activesupport', '~> 7.0.0'

group :test do
  gem 'mysql2', '>= 0.4.4'
  gem 'pg', '>= 1.1', '< 2.0'
  gem 'rspec', '~> 3.0'
  gem 'rspec-rails', '~> 6.1.0'
  gem 'sqlite3', '~> 1.4'
end

group :development do
  gem 'rubocop', '~> 1.0'
  gem 'rubocop-rspec'
end
